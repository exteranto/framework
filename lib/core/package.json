  "keywords": [
    "extension",
    "framework",
    "typescript",
    "exteranto",
    "application",
    "core"
  ],
  "author": {
    "name": "Pavel Koch",
    "email": "kouks.koch@gmail.com",
    "url": "https://pavelkoch.io"
  },
  "contributors": [
    {
      "name": "Michael Bausano",
      "email": "bausanomichal@gmai.com"
    }
  ],
  "repository": "github:exteranto/core",
  "license": "MIT",
  "dependencies": {
    "@bausano/data-structures": "^1.0.1",
    "reflect-metadata": "^0.1.12"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.29",
    "@zerollup/ts-transform-paths": "^1.7.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^5.1.1",
    "nyc": "^13.3.0",
    "sinon": "^7.2.7",
    "ts-mockito": "^2.3.1",
    "ts-node": "^8.0.2",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.13.1",
    "ttypescript": "^1.5.6",
    "typescript": "^2.9.2"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
